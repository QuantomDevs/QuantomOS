{
  "name": "quantomos",
  "version": "1.3.5",
  "description": "QuantomOS - An open-source user interface designed to manage your server and homelab",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "NODE_ENV=development PORT=2525 nodemon",
    "dev:frontend": "cd src/frontend && vite --host",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "NODE_ENV=production node esbuild.config.js",
    "build:frontend": "tsc -b tsconfig.frontend.json && vite build",
    "start": "NODE_ENV=production node dist/backend/index.js",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "eslint src/backend --config eslint.backend.config.mjs",
    "lint:frontend": "eslint src/frontend --config eslint.frontend.config.mjs",
    "clean": "rimraf node_modules package-lock.json dist",
    "docker:build:dev": "docker build . --no-cache -t ghcr.io/quantomdevs/quantomos:latest",
    "docker:build": "docker build --platform linux/amd64 . -t ghcr.io/quantomdevs/quantomos:latest",
    "docker:build:multi": "docker buildx build --platform linux/amd64,linux/arm64 . -t ghcr.io/quantomdevs/quantomos:latest",
    "docker:run": "docker run --platform=linux/amd64 -p 2022:80 -p 2525:2525 ghcr.io/quantomdevs/quantomos:latest",
    "docker:run:dev": "docker run -p 2022:2022 ghcr.io/quantomdevs/quantomos:latest"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@loganmarchione/homelab-svg-assets": "^0.4.14",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.3",
    "@mui/x-charts": "^7.25.0",
    "@types/react-grid-layout": "^1.3.5",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "framer-motion": "^12.4.7",
    "geo-tz": "^8.1.4",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mui-file-input": "^6.0.0",
    "multer": "^2.0.2",
    "nanoid": "^5.1.2",
    "node-ical": "^0.22.1",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.2",
    "react-hook-form": "^7.54.2",
    "react-hook-form-mui": "^7.5.0",
    "react-icons": "^5.4.0",
    "react-markdown": "^10.1.0",
    "react-remove-scroll": "^2.7.1",
    "react-router-dom": "^7.2.0",
    "react-virtualized": "^9.22.6",
    "shortid": "^2.2.17",
    "sweetalert2": "^11.16.0",
    "systeminformation": "^5.27.8",
    "wol": "^1.0.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-virtualized": "^9.22.2",
    "@types/shortid": "^2.2.0",
    "@types/wol": "^1.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.1.2",
    "esbuild": "^0.25.0",
    "esbuild-node-externals": "^1.18.0",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.19.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.18.2",
