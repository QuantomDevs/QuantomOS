{
  "name": "quantomos-frontend",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:dev": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "preview": "vite preview",
    "docker:build:dev": "docker build . -t ghcr.io/snenjih/quantomos-frontend:latest",
    "docker:build": "docker build --platform linux/amd64 . -t ghcr.io/snenjih/quantomos-frontend:latest",
    "docker:run": "docker run --platform=linux/amd64 -p 2022:80 -p 2525:2525 ghcr.io/snenjih/quantomos-frontend:latest",
    "docker:run:dev": "docker run -p 2022:80 -p 2525:2525 ghcr.io/snenjih/quantomos-frontend:latest",
    "docker:push": "docker push ghcr.io/snenjih/quantomos-frontend:latest",
    "docker:stop": "docker ps -q --filter 'ancestor=ghcr.io/snenjih/quantomos-frontend:latest' | xargs -r docker stop",
    "docker:rm": "docker ps -a -q --filter 'ancestor=ghcr.io/snenjih/quantomos-frontend:latest' | xargs -r docker rm",
    "docker:clean": "npm run docker:stop && npm run docker:rm",
    "docker:restart": "npm run docker:stop && npm run docker:rm && npm run docker:run"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.3",
    "@mui/x-charts": "^7.25.0",
    "axios": "^1.8.3",
    "framer-motion": "^12.4.7",
    "mui-file-input": "^6.0.0",
    "nanoid": "^5.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-hook-form-mui": "^7.5.0",
    "react-icons": "^5.4.0",
    "react-markdown": "^10.1.0",
    "react-remove-scroll": "^2.7.1",
    "react-router-dom": "^7.2.0",
    "react-virtualized": "^9.22.6",
    "shortid": "^2.2.17",
    "sweetalert2": "^11.16.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-virtualized": "^9.22.2",
    "@types/shortid": "^2.2.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.19.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "http-status-codes": "^2.3.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.2.2"
  }
}
